#Roguelike Dungeons
{
	# Very High (7), High (9), Medium (8), Low (12), Very Low (18)
	DungeonSpawnProb=9

	#If helpful features like brewing stands, ender chests, enchanting stations, anvils should generate.
	Generous=true
	#How far apart the rooms should be
	LevelScatter=10
	#The distance from the stairs dungeon levels will spread
	LevelRange=80
